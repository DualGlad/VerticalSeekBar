<?xml version="1.0" encoding="utf-8"?>

<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners
                android:radius="6dp" />
            <solid
                android:color="#FF000000" />
        </shape>
    </item>
    <item
        android:top="2dp"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp">
        <shape>
            <size
                android:height="26dp"
                android:width="16dp" />
            <corners
                android:radius="4dp" />
            <solid
                android:color="#FF222222" />
            <stroke
                android:color="#FFFFFFFF"
                android:width="2dp" />
        </shape>
    </item>
</layer-list>
